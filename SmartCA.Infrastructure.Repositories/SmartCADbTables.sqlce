    MobileAlias nvarchar(16) NULL,
    MobilePIN nvarchar(16) NULL,
    Email nvarchar(256) NULL,
    PasswordQuestion nvarchar(256) NULL,
    PasswordAnswer nvarchar(128) NULL,
    IsApproved bit DEFAULT(0) NOT NULL,
    IsLockedOut bit DEFAULT(0) NOT NULL,
    CreateDate datetime DEFAULT(GETDATE()) NOT NULL,
    LastLoginDate datetime DEFAULT('1/1/1753') NOT NULL,
    LastPasswordChangedDate datetime DEFAULT('1/1/1753') NOT NULL,
    LastLockoutDate datetime DEFAULT('1/1/1753') NOT NULL,
    LastActivityDate datetime DEFAULT(GETDATE()) NOT NULL,
    FailedPasswordAttemptCount int DEFAULT(0) NOT NULL,
    FailedPasswordAttemptWindowStart datetime DEFAULT('1/1/1753') NOT NULL,
    FailedPasswordAnswerAttemptCount int DEFAULT(0) NOT NULL,
    FailedPasswordAnswerAttemptWindowStart datetime DEFAULT('1/1/1753') NOT NULL,
    Comment nvarchar(1000) NULL
)
GO

